<h2>Sign In</h2>
<%= form_with url: sign_in_path do |f| %>
    <p>
        Email<br>
        <%= f.text_field :email, { :class => "form-control", id: 'email'} %>
    </p>
    <p>
        Password<br>
        <%= f.password_field :password, { :class => "form-control", id: 'password'} %>
    </p>
    <p>
        <%= f.submit 'Log In', { :class => "btn btn-success"} %>
    </p>
<% end %>